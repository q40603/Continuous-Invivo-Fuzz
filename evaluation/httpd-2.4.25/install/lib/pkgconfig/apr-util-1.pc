prefix=/root/eval/httpd-2.4.25/install
exec_prefix=/root/eval/httpd-2.4.25/install
libdir=${prefix}/lib
APRUTIL_MAJOR_VERSION=1
includedir=/root/eval/httpd-2.4.25/install/include

Name: APR Utils
Description: Companion library for APR
Version: 1.6.1
# assume that apr-util requires libapr of same major version
Requires: apr-1
Libs: -L${libdir} -laprutil-${APRUTIL_MAJOR_VERSION}  -lexpat
Cflags: -I${includedir}
